<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Flirteo.app" version="0.95.94" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Flirteo</name>
    <description>A Quasar Project</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <plugin name="cordova-plugin-customurlscheme" spec="^5.0.1">
        <variable name="URL_SCHEME" value="flirteo" />
    </plugin>
    <plugin name="cordova-plugin-fullscreen" spec="1.3.0" />
    <plugin name="cordova-plugin-purchase" spec="^13.11.1" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidInsecureFileModeEnabled" value="true" />
    <preference name="SafeArea" value="true" />
    <platform name="android">
        <preference name="AndroidWindowSplashScreenAnimatedIcon" value="res/android/screen/logo.xml" />
        <preference name="AndroidWindowSplashScreenBackground" value="#ffdd0f" />
        <icon density="ldpi" src="res/android/icon/logo-ldpi.png" />
        <icon density="mdpi" src="res/android/icon/logo-mdpi.png" />
        <icon density="hdpi" src="res/android/icon/logo-hdpi.png" />
        <icon density="xhdpi" src="res/android/icon/logo-xhdpi.png" />
        <icon density="xxhdpi" src="res/android/icon/logo-xxhdpi.png" />
        <icon density="xxxhdpi" src="res/android/icon/logo-xxxhdpi.png" />
        <config-file parent="/manifest" target="AndroidManifest.xml">
            <uses-permission android:name="com.android.vending.BILLING" />
        </config-file>
        <allow-intent href="market:*" />
        <allow-intent href="http://*/*" />
        <allow-intent href="https://*/*" />
        <allow-intent href="tel:*" />
        <allow-intent href="sms:*" />
        <allow-intent href="mailto:*" />
        <allow-intent href="geo:*" />
        <preference name="android-minSdkVersion" value="26" />
        <preference name="AndroidInAppBrowserStorageEnabled" value="true" />
    </platform>
    <platform name="ios">
        <preference name="scheme" value="app" />
        <preference name="hostname" value="localhost" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#000000" />
        <access origin="*" />
        <allow-intent href="*" />
        <icon height="20" src="res/ios/icon/icon-20.png" width="20" />
        <icon height="29" src="res/ios/icon/icon-29.png" width="29" />
        <icon height="40" src="res/ios/icon/icon-40.png" width="40" />
        <icon height="50" src="res/ios/icon/icon-50.png" width="50" />
        <icon height="57" src="res/ios/icon/icon-57.png" width="57" />
        <icon height="58" src="res/ios/icon/icon-58.png" width="58" />
        <icon height="60" src="res/ios/icon/icon-60.png" width="60" />
        <icon height="66" src="res/ios/icon/icon-66.png" width="66" />
        <icon height="72" src="res/ios/icon/icon-72.png" width="72" />
        <icon height="76" src="res/ios/icon/icon-76.png" width="76" />
        <icon height="80" src="res/ios/icon/icon-80.png" width="80" />
        <icon height="87" src="res/ios/icon/icon-87.png" width="87" />
        <icon height="100" src="res/ios/icon/icon-100.png" width="100" />
        <icon height="114" src="res/ios/icon/icon-114.png" width="114" />
        <icon height="120" src="res/ios/icon/icon-120.png" width="120" />
        <icon height="144" src="res/ios/icon/icon-144.png" width="144" />
        <icon height="152" src="res/ios/icon/icon-152.png" width="152" />
        <icon height="167" src="res/ios/icon/icon-167.png" width="167" />
        <icon height="180" src="res/ios/icon/icon-180.png" width="180" />
        <icon height="1024" src="res/ios/icon/icon-1024.png" width="1024" />
        <splash height="1136" src="res/ios/screen/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/ios/screen/Default-667h.png" width="750" />
        <splash height="2208" src="res/ios/screen/Default-736h.png" width="1242" />
        <splash height="1242" src="res/ios/screen/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" src="res/ios/screen/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="2048" src="res/ios/screen/Default-Landscape@~ipadpro.png" width="2732" />
        <splash height="768" src="res/ios/screen/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" src="res/ios/screen/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="2732" src="res/ios/screen/Default-Portrait@~ipadpro.png" width="2048" />
        <splash height="1024" src="res/ios/screen/Default-Portrait~ipad.png" width="768" />
        <splash height="960" src="res/ios/screen/Default@2x~iphone.png" width="640" />
        <splash height="2732" src="res/ios/screen/Default@2x~universal~anyany.png" width="2732" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
